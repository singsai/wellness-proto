<!DOCTYPE html>
<html>
<head>
  <title>WellnessProto</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if current_user %>
    <%= link_to 'Logout', logout_path %>
  <% else %>
    <%= link_to 'Login', login_path %>
  <% end %>
  
	<% flash.each do |key, value| %>
  	<div class="flash <%= key %> ">
  		<%= value %>
  	</div>	
  <% end %>
<%= yield %>

</body>
</html>
